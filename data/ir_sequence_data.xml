<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <!-- Sequences for treasury.incoming -->
        <record id="seq_treasury_incoming" model="ir.sequence">
            <field name="name">Treasury Incoming</field>
            <field name="code">treasury.incoming</field>
            <field name="prefix">INS-</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>
        <!-- Sequences for treasury.outgoing -->
        <record id="seq_treasury_outgoing" model="ir.sequence">
            <field name="name">Treasury Outgoing</field>
            <field name="code">treasury.outgoing</field>
            <field name="prefix">OTS-</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>

    </data>
</odoo>
